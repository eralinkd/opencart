	<link href="{{ components }}" rel="stylesheet" type="text/css" /> #}

	<link href="{{ reset }}" rel="stylesheet" type="text/css" />
	<link href="{{ index }}" rel="stylesheet" type="text/css" />
	<link href="{{ app_header }}" rel="stylesheet" type="text/css" />
	<link href="{{ page_home }}" rel="stylesheet" type="text/css" />
	{% if is_product_page %}
	<link href="{{ page_product }}" rel="stylesheet" type="text/css" />
	{% endif %}
	{% if is_cart_page %}
	<link href="{{ page_cart }}" rel="stylesheet" type="text/css" />
	{% endif %}
	<link href="{{ app_footer }}" rel="stylesheet" type="text/css" />
	{% if icon %}
	<link rel="icon" href="{{ icon }}" type="image/png">
	{% endif %}
	{% for style in styles %}
	<link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
	{% endfor %}
	{% for script in scripts %}
	<script src="{{ script.href }}" type="text/javascript"></script>
	{% endfor %}
</head>

